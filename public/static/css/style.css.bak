.list-pic { padding-bottom:15px; overflow:hidden; 
}
.list-pic li {width: 16.66%; float: left}
.list-pic li a {overflow: hidden; display: block; position: relative}
.list-pic li a img {width: 100%; -webkit-filter: grayscale(1)}
.list-pic li a img {transition:transform 0.3s ease-out; -webkit-transition: -webkit-transform .3s ease-out; -moz-transition: -moz-transform .3s ease-out; -o-transition: -o-transform .3s ease-out}
.list-pic2 li .pic img {transition:transform 0.3s ease-out; -webkit-transition: -webkit-transform .3s ease-out; -moz-transition: -moz-transform .3s ease-out; -o-transition: -o-transform .3s ease-out}
#hd {transition:transform 0.3s ease-out; -webkit-transition: -webkit-transform .3s ease-out; -moz-transition: -moz-transform .3s ease-out; -o-transition: -o-transform .3s ease-out}
.list-pic li a:hover img {transform: scale(1.1); -webkit-filter: grayscale(100); -webkit-transform: scale(1.1); -moz-transform: scale(1.1); -o-transform: scale(1.1)}
.list-pic2 li .pic a:hover img {transform: scale(1.1); -webkit-filter: grayscale(100); -webkit-transform: scale(1.1); -moz-transform: scale(1.1); -o-transform: scale(1.1)}
.list-pic li a span {background: rgb(0, 0, 0); padding: 10px 0px; left: 0px; width: 230px; text-align: center; bottom: -60px; color: rgb(255, 255, 255); position: absolute; opacity: 0.7; -moz-opacity: 0.7; -khtml-opacity: 0.7}
.list-pic2 li {width: 50%; height: 240px; padding-right: 10px; margin-bottom: 20px; float: left; box-sizing: border-box; -webkit-box-sizing: border-box; -moz-box-sizing: border-box}
.list-pic2 li .pic {border: 1px solid rgb(204, 204, 204); width: 220px; height: 220px; overflow: hidden; float: left}
.list-pic2 li .pic img {width: 220px}
.list-pic2 li .info {overflow: hidden; padding-left: 20px}
.list-pic2 li .info h3 {line-height: 1.5em; font-weight: normal; margin-bottom: 5px}
.list-pic2 li .info h3 a {color: rgb(0, 0, 0)}


