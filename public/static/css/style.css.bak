.plugthumb .bm {background: rgb(247, 249, 250); width:288px; height:166px; line-height: 40px; overflow: hidden;font:14px/40px "微软雅黑";}
.plugthumb .bm dl{ color:#999; padding:20px 20px 0 20px;}
.plugthumb .bm dl dt{font:12px/22px "微软雅黑"; padding-bottom:8px;border-bottom:solid 1px #e1e1e1;}
.plugthumb .bm dl dt a{ font: bold 14px/22px "微软雅黑"; color:#333; text-decoration:none;}
.plugthumb .bm dl dt a:hover{ color:#369;}
.plugthumb .bm dl dd{font:12px/22px "微软雅黑"; border-bottom:solid 1px #e1e1e1; padding:5px 0;}
.plugthumb .bm dl dd span{ display:block; float:left; height:30px;}
.plugthumb .tp figure {margin: 0px; width: 100%; height: 100%; text-align: center; overflow: hidden; display: inline-block; position: relative; z-index: 1; cursor: pointer}
.plugthumb .tp figure img {display: block; position: relative; opacity: 1}
.plugthumb .tp figure figcaption::after {pointer-events: none}
.plugthumb .tp figure figcaption::before {pointer-events: none}
.plugthumb .tp figure figcaption {left: 0px; top: 0px; width: 100%; height: 100%; position: absolute}
figcaption {box-sizing: border-box}
*::after {box-sizing: border-box}
*::before {box-sizing: border-box}
figure.effect-bubba {background: #333;}
figure.effect-bubba a img {transition:opacity 0.35s; opacity: 1; -webkit-transition: opacity .35s}
figure.effect-bubba:hover img {opacity: 0.5}
figure.effect-bubba:hover p img {text-align: center; opacity: 1}
figure.effect-bubba figcaption::after {transition:opacity 0.35s, transform 0.35s; left: 30px; top: 30px; right: 30px; bottom: 30px; position: absolute; content: ""; opacity: 0; -webkit-transition: opacity .35s, -webkit-transform .35s}
figure.effect-bubba figcaption::before {transition:opacity 0.35s, transform 0.35s; left: 30px; top: 30px; right: 30px; bottom: 30px; position: absolute; content: ""; opacity: 0; -webkit-transition: opacity .35s, -webkit-transform .35s}
figure.effect-bubba figcaption::before {border-top-color: rgb(255, 255, 255); border-bottom-color: rgb(255, 255, 255); border-top-width: 1px; border-bottom-width: 1px; border-top-style: solid; border-bottom-style: solid; transform: scale(0, 1); -webkit-transform: scale(0, 1)}
figure.effect-bubba figcaption::after {border-right-color: rgb(255, 255, 255); border-left-color: rgb(255, 255, 255); border-right-width: 1px; border-left-width: 1px; border-right-style: solid; border-left-style: solid; transform: scale(1, 0); -webkit-transform: scale(1, 0)}
figure.effect-bubba p {margin-top:35%; margin-left: 44%; position: relative; opacity: 0; transform: translate3d(0, 20px, 0);  -webkit-transform: translate3d(0, 20px, 0)}
figure.effect-bubba:hover figcaption::after {opacity: 1; transform: scale(1); -webkit-transform: scale(1)}
figure.effect-bubba:hover figcaption::before {opacity: 1; transform: scale(1); -webkit-transform: scale(1)}
figure.effect-bubba:hover h2 {opacity: 1; transform: translate3d(0, 0, 0); -webkit-transform: translate3d(0, 0, 0)}
figure.effect-bubba:hover p {opacity: 1; transform: translate3d(0, 0, 0); -webkit-transform: translate3d(0, 0, 0)}

