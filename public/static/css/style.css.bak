.header{height:75px;position: relative;width:100%;background: #fff;z-index: 999;}
.logo{float: left;margin-top: 11px;}
.nav-btn{width: 20px;height: 20px;display: none;cursor: pointer;position: absolute;right: 10px;top: 50%;margin-top: -10px;}
.current{position: fixed;height: 40px;top: 0;left: 0;background: #fff;z-index: 999;filter:alpha(opacity=96); 
-moz-opacity:0.96; 
opacity:0.96; border-bottom:solid 1px #e1e1e1;}
.current .logo{width:100px;margin-top: 3px;}
.current .logo img{vertical-align: top;border: none;max-width: 100%;}
.current .menu ul li a, .menu ul li a:visited {display:block; text-align:center; text-decoration:none; 
height:40px;border:1px solid #fff;border-width:1px 0px 0 0;line-height:40px; font-size:12px; width:140px; display:block;}
.current .menu a{line-height:40px;font-size:12px;}
.current .current .menu a:hover{ background:#097ec6;}
.current .menu ul li:hover ul {display:block; position:absolute; top:40px; left:0; width:140px;}
.current .menu ul li a.hide{ background:url(../images/jiantou_hui.png) no-repeat 110px 18px;}
.current .menu ul li a.hide:hover{ background:#097ec6;}
.current .menu ul li a.hide1{ background:url(../images/jiantou_hui.png) no-repeat 120px 18px;}
.current .menu ul li a.hide1:hover{ background:#097ec6;}
.current .menu ul li:hover a {color:#fff; background:#097ec6;}
.current .menu ul li.hover a {color:#fff;}
.current .menu ul li:hover ul {display:block; position:absolute; top:40px; left:0; width:140px;}
.current .menu ul li:hover ul li a {display:block; background:#e7f0f9; color:#000; height:40px; font:12px/40px "微软雅黑";}
.current .menu ul li:hover ul li a:hover {background:#cde1ee; color:#000;}

.navi_head{height:75px;background-color:#fff; float:right;}
.navi_body{overflow:hidden;height:75px;background:#fff;transition:height ease 0.5s;}
.current .navi_body {display:block; text-align:center; text-decoration:none; 
height:40px;line-height:40px;transition:height ease 0.5s;}
.navi_logo{ float:left;}
.navi_body:hover{height:245px;border-bottom:solid 1px #e1e1e1;}
.current .navi_body:hover{height:200px;border-bottom:solid 1px #e1e1e1;}
.navi_head>div>span{width:120px;text-align:center;height:300px;display:inline-block;color:#333;font-size:14px;vertical-align:top;}
.current .navi_head>div>span{width:120px;text-align:center;height:300px;display:inline-block;color:#333;font-size:12px;vertical-align:top;}
.navi_head>div>span>p a{text-decoration:none; font:12px/30px "微软雅黑";}
.current .navi_head>div>span>p a{text-decoration:none; font:12px/30px "微软雅黑";}
.navi_head>div>span>p a:hover{color:#097ec6;text-decoration: none;}
