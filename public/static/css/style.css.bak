#featureContainer {margin: 0px auto; width:1200px;height:120px; clear:both;}
#wrap {OVERFLOW: hidden}
#load {border-left: #999 0px dotted; TEXT-INDENT: -9999px; margin: 0px auto; width:1200px; DISPLAY: none;height: 100px;}
#feature {position: relative; border-left: #999 0px dotted;}
#feature .button {position: absolute;width:1080px; left: 0px}
#feature .prev {TEXT-INDENT: -9999px; width: 30px; DISPLAY: block; background: url(../images/feature_left.gif) center top; float: left; height: 48px;TOP:26px;left:0px;}
#feature .next {TEXT-INDENT: -9999px; width: 30px; DISPLAY: block; background: url(../images/feature_right.gif) center top; float: right; height: 48px; TOP:26px;right:0px;}
#feature .prev:hover {background-position: center bottom}
#feature .next:hover {background-position: center bottom}
.featureUL {position: relative;padding-bottom: 0px; LIST-STYLE-TYPE: none; margin: 0px; padding-left: 0px; width: 2800px; padding-right: 0px; float: left;padding-TOP: 0px; left: 0px}
.featureBox {border-left: #999 0px dotted; width:180px; background-REPEAT: no-repeat; float: left; height:120px; OVERFLOW: hidden;}
#botton-scroll {Z-INDEX: 0; border-left: #999 0px dotted; margin: 0px auto; width:1080px; float: left; height:120px; border-TOP: #999 0px dotted;}
#block {Z-INDEX: 0; position: relative; border-left: #999 0px dotted;margin: 0px auto; padding-left: 0px; width:1080px; padding-right: 0px; height: 120px; FONT-SIZE: 70%; OVERFLOW: hidden; border-TOP: #999 0px dotted;; padding-TOP: 0px}
#block .box {border-left: #d8d8d8 0px solid;margin: 0px 15px;width:150px; float: left; height: 150px; }
#block .box .feature-title {FONT-SIZE: 140%;}
#block .box .feature-thumb {width:150px; margin-bottom: 5px; float: left; height: 150px}


