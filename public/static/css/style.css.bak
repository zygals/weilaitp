.navi_head>div>span>p.navi_title a:hover{color:#097ec6;t}
.current .navi_head>div>span>p.navi_title a{font:14px/40px "微软雅黑";line-height:40px;margin-top:0;color:#333;}
.current .navi_head>div>span>p.navi_title a:hover{color:#097ec6;t}
.navi_title{ font:16px/75px "微软雅黑";line-height:75px;margin-top:0;}
.current .navi_title{font-size:14px;line-height:40px;margin-top:0;}
.navi_head>div>span:hover{}


